{
  "name": "proper-build",
  "version": "1.0.1",
  "description": "Proper Design's build package",
  "main": "''",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --host 0.0.0.0 --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "scss": "node-sass --output-style compressed -o dist/css src/scss",
    "build:css": "npm run scss && npm run autoprefixer",
    "waitawhile" : "sleep 100"
  },
  "repository": {
    "type": "git",
    "url": "propergit:.git"
  },
  "author": "Proper Design",
  "license": "BSD-2-Clause",
  "dependencies": {
    "jquery" : "latest",
    "fitvids": "latest",
    "squishMenu" : "latest",
    "jquery.cookie": "~1.4.1",
    "npm-modernizr" : "latest",
    "js-cookie" : "latest"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "creeperyang/babel-loader",
    "babel-loader": "6.2.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-exists": "^2.0.0",
    "sass-loader": "^4.0.0",
    "node-sass": "^3.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "postcss-cli": "^2.6.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "1.3.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-cleanup-plugin": "^0.2.0",
    "webpack-dev-server": "1.14.1",
    "write-file-webpack-plugin": "3.1.8",
    "browser-sync-webpack-plugin": "1.1.2",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "keywords": [
    "starter",
    "blank",
    "basic"
  ]
}