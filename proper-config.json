{
  "build": {
    "source": "themes/Proper-Bear-master",
    "bower": {
      "installDest": "bower_components",
      "cssDestName": "thirdparty.css",
      "cssDestDir": "_/css",
      "cssMinDestName": "thirdparty.min.css",
      "cssMinDestDir": "_/css",
      "jsDestName": "thirdparty.js",
      "jsDestDir": "_/js",
      "jsMinDestName": "thirdparty.min.js",
      "jsMinDestDir": "_/js"
    },
    "svg": {
      "srcDir": "_/svg/src",
      "destName": "symbols.svg",
      "destDir": "_/svg",
      "svgDocumentSettings": {
        "xmlDeclaration": true,
        "namespaceIDs": false,
        "doctypeDeclaration": true
      }
    },
    "js": {
      "srcDir": "_/js/src",
      "entrypoint": "index.js",
      "destName": "bundle.js",
      "destDir": "_/js"
    },
    "sass": {
      "srcDir": "_/scss",
      "destDir": ""
    }
  },
  "plugins" : [
  ],
  "themes" : [
  ]
}